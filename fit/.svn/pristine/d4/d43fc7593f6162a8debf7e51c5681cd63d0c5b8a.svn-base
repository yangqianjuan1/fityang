*{
    margin: 0;
    padding: 0;
}
/* * { touch-action: pan-y; }  */
body,html{
    height: 100%;
    width: 100%;
    font-family: "Helvetica Neue",Helvetica,"PingFang SC","Hiragino Sans GB","Microsoft YaHei","微软雅黑",Arial,sans-serif;
    background: #f3f4f9;
    font-size: 14px;
}
li {list-style-type:none;}

.box_s{
    box-sizing: border-box;
}
.page{
    height: 100%;
    width: 100%;
}
.bor1{
    border-radius: 1px;
}
.bor2{
    border-radius: 2px;
}
.bor3{
    border-radius: 3px;
}
.bor4{
    border-radius: 4px;
}
.bor5{
    border-radius: 5px;
}
.bor6{
    border-radius: 6px;
}
.bor7{
    border-radius: 7px;
}
.bor8{
    border-radius: 8px;
}
.bor9{
    border-radius: 9px;
}
.bor10{
    border-radius: 10px;
}



.ellipse{
    border-top-left-radius: 9999px;
    border-top-right-radius: 9999px;
    border-bottom-left-radius: 9999px;
    border-bottom-right-radius: 9999px
}

a{
    text-decoration: none;
}


.clear{ clear:both} 

::-webkit-scrollbar{
    width: 10px;
    height: 10px;
    background-color: #f5f7fa;
  }
  /*滚动条的轨道*/
  ::-webkit-scrollbar-track{
    /*box-shadow: inset 0 0 5px rgba(0,0,0,.3);*/
    background-color: #f5f7fa;
   
  }
  /*滚动条的滑块按钮*/
  ::-webkit-scrollbar-thumb{
    border-radius: 10px;
    background-color: #e4e7ed;
    /*box-shadow: inset 0 0 5px #000;*/
  }
  /*滚动条的上下两端的按钮*/
  ::-webkit-scrollbar-button{
    height: 0px;
    background-color: #B0AEDA;
  }
  .el_tableDiv{
    height: 420px;
    overflow-y:auto;
  }

  .mapreturn{
      cursor: pointer;
      border: 1px solid #1E9FFF;
      background-color:#1E9FFF;
      color: #fff;
      padding: 5px 10px;
      border-radius: 10px;
  }

  .info-box {
    padding: 12px;
    font-size: 14px;
    line-height: 20px;
    color: #fff;
    background: rgba(36, 110, 221, .6);
    border-radius: 5px;
}