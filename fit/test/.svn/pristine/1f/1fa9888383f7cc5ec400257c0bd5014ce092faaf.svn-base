*{
  -webkit-touch-callout:inherit;

}
html{
  background: #ffffff;
  margin: 0 auto;
}
button:active{/*点击效果*/   /* For Firefox3.6+ */
  -webkit-box-shadow:0 0 15px #ffffff inset;            /* For Chrome5+, Safari5+ */
}
body{

  box-shadow: 0 0 100px #F4F4F8;
  -webkit-box-shadow: 0 0 100px #F4F4F8;
  -moz-box-shadow: 0 0 100px #F4F4F8;

  color: #502111;
  margin:  0 auto;
  background-image:url("../assets/img/index/index.png");
  background-size:100% 100% ;
  background-repeat: no-repeat;
}
.noStroke{
  -webkit-text-stroke: 0 #384248;
}
html,body{
  width: 100%;
  height:100%;
}
.clear:after{
  content: '';
  display: block;
  clear: both;
}

.clear{
  zoom:1;
}

img{
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.margin{
  margin: 0 auto;
}

.left{
