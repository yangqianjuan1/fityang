}


/*//定位上下左右居中*/
/*//定位左右居中*/
.ct {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.cl {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
}
.takeCenter{
  position: absolute;
  left: 50%;
  top: 50%;
  transform:  translate(-50%,-50%);
}
